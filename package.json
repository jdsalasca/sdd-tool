{
  "name": "sdd-cli",
  "version": "0.1.5",
  "description": "SDD-first, AI-native CLI for end-to-end delivery.",
  "homepage": "https://github.com/jdsalasca/sdd-tool#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdsalasca/sdd-tool.git"
  },
